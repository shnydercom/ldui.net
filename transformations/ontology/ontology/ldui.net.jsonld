			},
			"schema:rangeIncludes": {
				"@id": "schema:"
			},
			"rdfs:comment": "An A-version is one of the two versions tested in an A/B-Test. The A-version is usually the status quo of the application, and the B-version the changed one that's hoped to bring improved conversion.",
			"rdfs:label": "A-version"
		},
