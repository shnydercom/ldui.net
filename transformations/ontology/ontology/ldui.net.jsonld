{
	"@context": {
		"ldui": "http://ldui.net/ontology/",
		"schema": "http://schema.org/",
		"rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
		"rdfs": "http://www.w3.org/2000/01/rdf-schema#",
		"defines": {
			"@reverse": "rdfs:isDefinedBy"
		},
		"comment": "rdfs:comment",
		"label": "rdfs:label",
		"domain": {
			"@id": "rdfs:domain",
			"@type": "@id"
		},
		"range": {
			"@id": "rdfs:range",
			"@type": "@id"
		},
		"subClassOf": {
			"@id": "rdfs:subClassOf",
			"@type": "@id",
			"@container": "@set"
		},
		"subPropertyOf": {
			"@id": "rdfs:subPropertyOf",
			"@type": "@id",
			"@container": "@set"
		},
		"seeAlso": {
			"@id": "rdfs:seeAlso",
			"@type": "@id"
		}
	},
	"@id": "http://ldui.net/ontology",
	"defines": [{
			"@id": "schema:Thing",
			"@type": "rdfs:Class",
			"rdfs:comment": "The most generic type of item. Duplicate of schema:Thing-ontology, deprecate as soon as possible",
			"rdfs:label": "Thing"
		},
		{
			"@id": "ldui:Element",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
				"@id": "rdfs:Class"
			}],
			"rdfs:comment": "The root for all Classes in the ldui ontology, both as part of the UI or not",
			"rdfs:label": "element"
		},
		{
			"@id": "ldui:DataType",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The root class for all DataTypes. E.g. on/off -> true/false in the case of BooleanType",
			"rdfs:label": "data type"
		},
		{
			"@id": "ldui:DataTypeProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The root class for all DataTypeProperties, describing a DataType more clearly. E.g. minValue/currentValue/maxValue in the case of NumericTypeProperties",
			"rdfs:label": "data type property"
		},
		{
			"@id": "ldui:UIElement",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The root class for all User Interface elements",
			"rdfs:label": "UI element"
		},
		{
			"@id": "ldui:Action",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The root Action class, more detailed than schema:Action. Can be as specific as a ClickAction, or similar to a schema:CommentAction",
			"rdfs:label": "action"
		},
		{
			"@id": "ldui:State",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The state of a UIElement",
			"rdfs:label": "state"
		},
		{
			"@id": "ldui:SemanticFlag",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The root for flags that add semantic information by a simple flag. E.g. isHelp, isDense, isMini, isOverlay, isModal",
			"rdfs:label": "semantic flag"
		},
		{
			"@id": "ldui:SemanticProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "The root for properties that add semantic information about parts of a UI, e.g. result, menu, role, group",
			"rdfs:label": "semantic property"
		},

		{
			"@id": "ldui:PropertyValueSpecification",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "similar to schema:PropertyValueSpecification, but contains only a subset of properties. Some of the properties went into NumericTypeProperty and others",
			"rdfs:label": "property value specification"
		},
		{
			"@id": "ldui:BooleanType",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:DataType"
				}
			],
			"comment": "Data type: Boolean. Human-readable terms like on/off, true/false have this as a supertype",
			"rdfs:label": "boolean type"
		},
		{
			"@id": "ldui:NumericTypeProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:DataTypeProperty"
				}
			],
			"comment": "Human-readable numeric values can have min/max/current properties to describe them more closely",
			"rdfs:label": "numeric type property"
		},
		{
			"@id": "ldui:TextTypeProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:DataTypeProperty"
				}
			],
			"comment": "Human-readable text values can have properties such as valueMinLength and valueMaxLength to describe them more closely",
			"rdfs:label": "text type property"
		},
		{
			"@id": "ldui:MediaDataType",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:DataType"
				}
			],
			"comment": "Data types that are not part of html, but can be embedded, such as image, audio and video",
			"rdfs:label": "media data type"
		},
		{
			"@id": "ldui:MediaDataTypeProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:DataTypeProperty"
				}
			],
			"comment": "Properties of a ldui:MediaType, like isPlaying",
			"rdfs:label": "media data type property"
		},
		{
			"@id": "ldui:VisualExpression",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "A visual form of expressing relations of UI elements to each other. E.g. color, italic, underlined, background, highlight",
			"rdfs:label": "visual expression"
		},
		{
			"@id": "ldui:Some",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "When multiple options are present, ldui:Some is a super-class for a number of items out of a whole. E.g.: selectedIndex, activeTabIndex, unselectedSet",
			"rdfs:label": "some"
		},
		{
			"@id": "ldui:UserActionState",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:State"
				}
			],
			"comment": "When the user interacts with the application, the application changes its state as a consequence",
			"rdfs:label": "user action state"
		},
		{
			"@id": "ldui:UserAction",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Action"
				}
			],
			"comment": "The root action class for all minor interactions users have with the interface (click, scroll, key-press, etc.)",
			"rdfs:label": "user action"
		},
		{
			"@id": "ldui:DeviceSpecificAction",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UserAction"
				}
			],
			"comment": "The root Action class for any interactions that are specific to the device type. E.g. KeyDownAction, ChangeOrientationAction",
			"rdfs:label": "device specific action"
		},
		{
			"@id": "ldui:MediaAction",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UserAction"
				}
			],
			"comment": "Actions that the user can do with media which is embedded in html, like RecordAction, DownloadAction, StartAction, PauseAction",
			"rdfs:label": "media action"
		},
		{
			"@id": "ldui:MediaPartAction",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UserAction"
				}
			],
			"comment": "Actions for part of a medium, e.g. AnnotateAction for an interesting part of a recording",
			"rdfs:label": "media part action"
		},
		{
			"@id": "ldui:UIElementState",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:State"
				}
			],
			"comment": "When the user interacts with an UI element, that element changes its state as a consequence",
			"rdfs:label": "ui element state"
		},
		{
			"@id": "ldui:UIStack",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElementState"
				}
			],
			"comment": "The layers of state as understood by the UI stack: https://www.scotthurff.com/posts/why-your-user-interface-is-awkward-youre-ignoring-the-ui-stack/#theuistack",
			"rdfs:label": "UI stack"
		},
		{
			"@id": "ldui:UIElementTransitionState",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElementState"
				}
			],
			"comment": "A UI element is in a transition state when it is entering another state over time. This can be useful for opening and closing animations, or skeleton states",
			"rdfs:label": "UI element transition state"
		},
		{
			"@id": "ldui:IssueSeverity",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "Priority or importance are both ways to express the severity of an issue",
			"rdfs:label": "issue severity"
		},
		{
			"@id": "ldui:IssueSeverityType",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:Element"
				}
			],
			"comment": "Types to categorize the severity of an issue in the UI, e.g. Warning, Error, Incomplete, Empty",
			"rdfs:label": "issue severity type"
		},
		{
			"@id": "ldui:AtomUIElement",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElement"
				}
			],
			"comment": "An Atom as understood by the Atomic Design Methodology: https://atomicdesign.bradfrost.com",
			"rdfs:label": "atom UI element"
		},
		{
			"@id": "ldui:Shape",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:AtomUIElement"
				}
			],
			"comment": "A basic shape like circle, triangle, rectangle etc",
			"rdfs:label": "shape"
		},
		{
			"@id": "ldui:MoleculeUIElement",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElement"
				}
			],
			"comment": "A Molecule as understood by the Atomic Design Methodology: https://atomicdesign.bradfrost.com",
			"rdfs:label": "molecule ui element"
		},
		{
			"@id": "ldui:OrganismUIElement",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElement"
				}
			],
			"comment": "An Organism as understood by the Atomic Design Methodology: https://atomicdesign.bradfrost.com",
			"rdfs:label": "organism ui element"
		},
		{
			"@id": "ldui:ContainerUIElement",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElement"
				}
			],
			"comment": "A container is a UI element which contains sub-elements",
			"rdfs:label": "container ui element"
		},
		{
			"@id": "ldui:StructuralUIElement",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UIElement"
				}
			],
			"comment": "Structural UI elements express structure of elements in the user interface, e.g. ABTest, Form, Template, Placeholder, Autocomplete",
			"rdfs:label": "structural ui element"
		},
		{
			"@id": "ldui:variation",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:ABTest"
			},
			"schema:rangeIncludes": {
				"@id": "ldui:UIElement"
			},
			"rdfs:comment": "variation of the same UI element, e.g. the A and B version of an A/B-test",
			"rdfs:label": "variation"
		},
		{
			"@id": "ldui:triggers",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:Button"
			},
			"schema:rangeIncludes": {
				"@id": "ldui:UserInteraction"
			},
			"rdfs:comment": "The (User-inter)-Action that gets triggered by a Button",
			"rdfs:label": "triggers"
		},
		{
			"@id": "ldui:slot",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:Container"
			},
			"schema:rangeIncludes": {
				"@id": "ldui:UIElement"
			},
			"rdfs:comment": "A slot is a space on a container for sub-elements",
			"rdfs:label": "slot"
		},
		{
			"@id": "ldui:a11y",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:Element"
			},
			"schema:rangeIncludes": {
				"@id": "ldui:Element"
			},
			"rdfs:comment": "Properties related to accessibility",
			"rdfs:label": "a11y"
		},
		{
			"@id": "ldui:CommunicateAction",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:UserAction"
				}
			],
			"comment": "Actions where two users exchange information, e.g. CommentAction, ReplyAction, ReactAction",
			"rdfs:label": "communicate action"
		},
		{
			"@id": "ldui:HierarchyFlag",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:SemanticFlag"
				}
			],
			"comment": "Flags that express a hierarchical position on an element",
			"rdfs:label": "hierarchical flag"
		},
		{
			"@id": "ldui:DescriptionFlag",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:SemanticFlag"
				}
			],
			"comment": "Flags to express that a UI element describes something, e.g. isHelp, isContent, isProgress",
			"rdfs:label": "descriptive flag"
		},
		{
			"@id": "ldui:OutcomeProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:SemanticFlag"
				}
			],
			"comment": "A property that holds a result, output or status",
			"rdfs:label": "outcome property"
		},
		{
			"@id": "ldui:NavigationProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:SemanticFlag"
				}
			],
			"comment": "A property that holds the navigation on a UI element, e.g. menu, settings",
			"rdfs:label": "navigation property"
		},
		{
			"@id": "ldui:ContextProperty",
			"@type": "rdfs:Class",
			"rdfs:subClassOf": [{
					"@id": "rdfs:Class"
				},
				{
					"@id": "ldui:SemanticFlag"
				}
			],
			"comment": "A property that holds the context a UI element is in, e.g. role, user, group",
			"rdfs:label": "context property"
		},

		{
			"@id": "ldui:multipleValues",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:PropertyValueSpecification"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether multiple values are allowed for the property. Default is false.",
			"rdfs:label": "multiple values"
		},
		{
			"@id": "ldui:valueName",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:PropertyValueSpecification"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Text"
			},
			"rdfs:comment": "Indicates the name of the PropertyValueSpecification to be used in URL templates and form encoding in a manner analogous to HTML's input@name.",
			"rdfs:label": "value name"
		},
		{
			"@id": "ldui:defaultValue",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:PropertyValueSpecification"
			},
			"schema:rangeIncludes": [{
				"@id": "schema:Text"
			}, {
				"@id": "schema:Thing"
			}],
			"rdfs:comment": "The default value of the input. For properties that expect a literal, the default is a literal value, for properties that expect an object, it's an ID reference to one of the current values.",
			"rdfs:label": "default value"
		},
		{
			"@id": "ldui:valueRequired",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:PropertyValueSpecification"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether the property must be filled in to complete the action. Default is false.",
			"rdfs:label": "value required"
		},
		{
			"@id": "ldui:readonlyValue",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:PropertyValueSpecification"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether or not a property is mutable. Default is false. Specifying this for a property that also has a value makes it act similar to a 'hidden' input in an HTML form.",
			"rdfs:label": "read-only value"
		},

		{
			"@id": "ldui:isActive",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:BooleanType"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether or not the element is active",
			"rdfs:label": "is active"
		},
		{
			"@id": "ldui:isInactive",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:BooleanType"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether or not the element is inactive",
			"rdfs:label": "is inactive"
		},
		{
			"@id": "ldui:isOn",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:BooleanType"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether or not the element is on",
			"rdfs:label": "is on"
		},

		{
			"@id": "ldui:isOff",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:BooleanType"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether or not the element is off",
			"rdfs:label": "is off"
		},
		{
			"@id": "ldui:isChecked",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:BooleanType"
			},
			"schema:rangeIncludes": {
				"@id": "schema:Boolean"
			},
			"rdfs:comment": "Whether or not the element is checked",
			"rdfs:label": "is checked"
		},
		{
			"@id": "ldui:isUnchecked",
			"@type": "rdf:Property",
			"schema:domainIncludes": {
				"@id": "ldui:BooleanType"
			},
